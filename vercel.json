{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {},
  "outputDirectory": ".",
  "installCommand": "npm install",
  "buildCommand": "echo 'Static build complete'",
  "devCommand": "npm run dev"
}
